#include <stdio.h>
int main(){
	int x;
	  printf("Enter a value between 1 through 5: ");
	  scanf("%d", &x);
	  switch (x){
	  	case 1:
	  		printf (" Food item - Chicken Gnocchi\n Price - Rs 748\n");
	  		break;
	  	case 2:
	  		printf (" Food item - Salad\n Price - Rs 437\n");
	  		break;
	    case 3: 
		    printf (" Food item - Tacos\n Price - Rs 592\n");
		    break;
	    case 4: 
		    printf (" Food item - Lobster\n Price - Rs 698\n");
		    break;
	    case 5: 
		    printf (" Food item - Zuppa Toscana\n Price - Rs 385\n");  
			break;  
		default: 
		    printf (" Selected number not found.\n Make sure the number is between 1-5. Please try again.");
		    break;
	  }
}
